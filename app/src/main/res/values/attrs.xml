<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="swipeButton">
        <attr name="btn_text" format="string" />
        <attr name="text_color" format="color" />
        <attr name="bg_color" format="color" />
        <attr name="arrow_color" format="color" />
        <attr name="btn_radius" format="integer" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MaterialSpinner">
        <attr name="emptyText" format="reference|string" />
    </declare-styleable>
</resources>